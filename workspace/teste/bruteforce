library(Biostrings)

DNA <- DNAStringSet(c("ACCTAGGTGT", "GTAGTACCTG", "GTAACCTGAG"))
s <- c(1,1,1)

score <- function(s,DNA)
{
		
}
bruteForce <- function(DNA,t,n,l)
{
	bestMotif <- 0
	bestScore <- 0
	for (i in 1:4)
	{
		for (i in 1:7)
		{
			if(score(s,DNA) > bestScore)
			{
				bestScore <- score(s,DNA)
				bestMotif <- conj
			}
		}
	}
	return(bestMotif)
}

b <- BString("I store any set of characters. The other XString objects store only the IUPAC characters for each biosequence type.")
d <- DNAString("GCATAT-TAC") # Creates DNAString object.
r <- RNAString("GCAUAU-UAC") # Creates RNAString object.
r <- RNAString(d) # Converts d into RNAString object.
p <- AAString("HCWYHH")





bruteForce <- function(DNA,t,n,l)
{
	bestMotif <- 0
	bestScore <- 0
	for (i in 1:3)# for each sequences Si in S 
	{
		for (i in 1:7)# (n-1)+1
		{
			if(score(s,DNA) > bestScore)
			{
				bestScore <- score(s,DNA)
				bestMotif <- conj
			}
		}
	}
	return(bestMotif)
}

b <- BString("I store any set of characters. The other XString objects store only the IUPAC characters for each biosequence type.")
d <- DNAString("GCATAT-TAC") # Creates DNAString object.
r <- RNAString("GCAUAU-UAC") # Creates RNAString object.
r <- RNAString(d) # Converts d into RNAString object.
